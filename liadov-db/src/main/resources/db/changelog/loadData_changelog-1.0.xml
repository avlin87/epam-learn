<?xml version="1.0" encoding="ISO-8859-1"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet author="Aleksandr Liadov" id="loadData_changelog-1.0">
        <loadData catalogName="CustomerData"
                  commentLineStartsWith="//"
                  file="loadData/CustomerData.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="liadov"
                  separator=","
                  tableName="Customer"
                  usePreparedStatements="true">
            <column header="customername"
                    name="CustomerName"
                    type="STRING"/>
            <column header="phone"
                    name="Phone"
                    type="STRING"/>
        </loadData>
        <loadData catalogName="orderData"
                  commentLineStartsWith="//"
                  file="loadData/OrderData.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="liadov"
                  separator=","
                  tableName="order"
                  usePreparedStatements="true">
            <column header="ordernumber"
                    name="OrderNumber"
                    type="STRING"/>
            <column header="customerid"
                    name="CustomerId"
                    type="NUMERIC"/>
            <column header="orderdate"
                    name="OrderDate"
                    type="Date"/>
            <column header="totalamount"
                    name="TotalAmount"
                    type="NUMERIC"/>
        </loadData>
        <loadData catalogName="SupplierData"
                  commentLineStartsWith="//"
                  file="loadData/SupplierData.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="liadov"
                  separator=","
                  tableName="Supplier"
                  usePreparedStatements="true">
            <column header="companyname"
                    name="CompanyName"
                    type="STRING"/>
            <column header="phone"
                    name="Phone"
                    type="STRING"/>
        </loadData>
        <loadData catalogName="ProductData"
                  commentLineStartsWith="//"
                  file="loadData/ProductData.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="liadov"
                  separator=","
                  tableName="Product"
                  usePreparedStatements="true">
            <column header="productname"
                    name="ProductName"
                    type="STRING"/>
            <column header="supplierid"
                    name="SupplierId"
                    type="NUMERIC"/>
            <column header="unitprice"
                    name="UnitPrice"
                    type="NUMERIC"/>
            <column header="isdiscontinued"
                    name="IsDiscontinued"
                    type="BOOLEAN"/>
        </loadData>
        <loadData catalogName="OrderProductData"
                  commentLineStartsWith="//"
                  file="loadData/OrderProductData.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="liadov"
                  separator=","
                  tableName="OrderProduct"
                  usePreparedStatements="true">
            <column header="orderid"
                    name="OrderId"
                    type="NUMERIC"/>
            <column header="productid"
                    name="ProductId"
                    type="NUMERIC"/>
        </loadData>
    </changeSet>
</databaseChangeLog>